{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":dependencyDashboard",
    ":disableRateLimiting",
    "docker:enableMajor",
    "docker:pinDigests",
    ":enablePreCommit",
    "github>OGKevin/renovate-config:commit-message",
    "github>OGKevin/renovate-config:pr-labels",
    "github>OGKevin/renovate-config:semantic-commits",
    "github>OGKevin/renovate-config:generic-regex-manager.json5",
    "github>OGKevin/renovate-config:semantic-commits",
    "github>OGKevin/renovate-config:automerge-docker-digest"
  ],
  "onboarding": true,
  "requireConfig": "required",
  "timezone": "Europe/Amsterdam",
  "dependencyDashboardTitle": "Renovate Dashboard",
  "suppressNotifications": [
    "prIgnoreNotification"
  ],
  "commitBodyTable": true,
  "rebaseWhen": "auto"
}
